}}, {"value": "<style>@media only screen and (max-width: 767px) and (orientation: portrait) {\n  .brand-bar {\n  \twidth: 100%;\n   \tbackground: white;\n   \tz-index: 500;\n  }\n  body[style] {\n    position: relative !important;\n    margin: auto auto !important;\n  }\n}</style>", "selector": "head", "dependencies": [], "type": "append", "id": "87151008f46a484499af453bddb9f0f1"}]}], "name": "Always Sticky - Home & Category"}, {"id": "15910350362", "actions": [{"viewId": "14735490990", "changes": []}, {"viewId": "14857820046", "changes": [{"dependencies": [], "type": "custom_code", "id": "7cff31b67cd24e38aaf13f23141a9a97", "value": function($){if(window.screen.width <= 767 && (window.orientation === 0 || window.orientation === 180)) {
  var utils = optimizely.get("utils");
  var lastScrollTop = 0;
  var waiting = false, endScrollHandle;
  var scroll = function () {
    utils.observeSelector('.hamburger-nav-layer', function(e){e.style.top=0;});
    var st = window.pageYOffset || document.documentElement.scrollTop;
    var elem = document.querySelector('.brand-bar');
    if(window.pageYOffset>40 && st<lastScrollTop) {
      elem.style.top = 0;
      elem.style.position = 'fixed';  	 
    } else {
      elem.style.removeProperty('top');
      elem.style.position = 'relative';
    }
    lastScrollTop = st <= 0 ? 0 : st;
	};
  utils.waitForElement('.brand-bar').then(function(){
    window.addEventListener("scroll", function(){
      if (waiting) {
        return;
      }
      waiting = true;
      // clear previous scheduled endScrollHandle
      clearTimeout(endScrollHandle);
      scroll();
      setTimeout(function () {
          waiting = false;
      }, 200);
      // schedule an extra execution of scroll() after 400ms
      // in case the scrolling stops in next 200ms
      endScrollHandle = setTimeout(function () {
        scroll();
      }, 400);
    });
 });
}
